{
  "name": "dogonauts-content-agent",
  "version": "1.1.0",
  "description": "Closed-loop content agent for Dogonauts e-commerce",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.json src/index.ts",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test:prompts": "tsx src/scripts/test-prompts.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["ai", "automation", "content-agent", "e-commerce"],
  "author": "Genflowautomation",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "express": "^4.18.2",
    "openai": "^4.20.1",
    "sharp": "^0.33.1",
    "node-schedule": "^2.1.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/node-schedule": "^2.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}